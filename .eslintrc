{
    "env": {
        "browser": true,
        "amd": true,
        "jquery": true
    },
    "rules": {
        "camelcase": [2, {"properties": "always"}],
        "curly": 2,
        "eqeqeq": [2, "smart"],
        "indent": 2,
        "new-cap": 2,
        "no-caller": 2,
        "no-new": 0,
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "quotes": [2, "single"],
        "radix": 2,
        "semi": [2, "always"],
        "strict": 2,
        "wrap-iife": [2, "inside"]
    }
}
